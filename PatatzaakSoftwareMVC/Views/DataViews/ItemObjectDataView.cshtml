@model List<Item>

@{
    ViewData["Title"] = "Item Object Data";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Item Object Data</title>
</head>
<body>
    <h1>Item Object Data</h1>

    <div class="item-list">
        <div class="item-row objectItemViewRow">
            <div class="col objectItemViewCol">
                <p>ItemId</p>
            </div>
            <div class="col objectItemViewCol">
                <p>ItemName</p>
            </div>
            <div class="col objectItemViewCol">
                <p>ItemImagePath</p>
            </div>
            <div class="col objectItemViewCol">
                <p>ItemPrice</p>
            </div>
            <div class="col objectItemViewCol">
                <p>ItemDiscount</p>
            </div>
        </div>
        @foreach (var item in Model)
        {
            <div class="item-row objectItemViewRow">
                <div class="col objectItemViewCol">
                    <p>@item.Id</p>
                </div>
                <div class="col objectItemViewCol">
                    <p>@item.Name</p>
                </div>
                <div class="col objectItemViewCol">
                    <p>@item.ImagePath</p>
                </div>
                <div class="col objectItemViewCol">
                    <p>@item.Price</p>
                </div>
                <div class="col objectItemViewCol">
                    <p>@item.Discount</p>
                </div>
            </div>
            <div class="box" style="height: 1px; background-color: black; width:100%; padding-left:10%; padding-right:10%"></div>               
        }
    </div>
</body>
</html>